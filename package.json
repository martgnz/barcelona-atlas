{
  "name": "bcn-atlas",
  "version": "0.1.0",
  "description": "Pre-built TopoJSON from CartoBCN",
  "scripts": {
    "prepublish": "bash prepublish",
    "postpublish": "git push && git push --tags && cd ../martgnz.github.io && git pull && cp -v ../bcn-atlas/bcn/census_tracts.json bcn-tracts.v1.json && git add bcn-tracts.v1.json && git commit -m \"bcn-atlas ${npm_package_version}\" && git push && cd -"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martgnz/bcn-atlas.git"
  },
  "keywords": [
    "topojson",
    "geojson",
    "shapefile",
    "barcelona"
  ],
  "author": {
      "name": "Martín González",
      "url": "https://martingonzalez.net"
  },
  "license": "MIT",
  "devDependencies": {
    "d3-dsv": "^1.0.3",
    "d3-geo-projection": "^1.2.1",
    "ndjson-cli": "^0.3.0",
    "shapefile": "^0.5.9",
    "topojson-client": "^2.1.0",
    "topojson-server": "^2.0.0",
    "topojson-simplify": "^2.0.0"
  }
}
